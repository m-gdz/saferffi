{
  "name": "oxidize",
  "displayName": "oxidize",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.87.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/m-gdz/saferffi"
  },
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "oxidize.refactor.wrap",
        "title": "Wrap Refactor",
        "category": "SaferFFI"
      },
      {
        "command": "oxidize.refactor.config",
        "title": "Config Refactor",
        "category": "SaferFFI"
      },
      {
        "command": "oxidize.refactor.removeunsafe",
        "title": "Remove Unsafe Refactor",
        "category": "SaferFFI"
      },
      {
        "command": "oxidize.launchRascal",
        "title": "Launch Rascal",
        "category": "SaferFFI"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile && npm run copy-rascal",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./ && npm run copy-rascal",
    "pretest": "npm run compile && npm run lint && npm run copy-rascal",
    "lint": "eslint src --ext ts",
    "test": "vscode-test",
    "copy-rascal": "copyfiles -u 3 '../../src/main/rascal/**/*' 'resources'"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.87.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/vsce": "3.1.0"
  }
}
